<html>

    <body>
        <% if (message==1){%>
            <dialog open>
                <center>
                    <h3>
                        Powodzenie!
                    </h3>
                </center>
                <center>
                    <p>
                        Plik został usunięty z kubełka!
                    </p>
                </center>
                <center>
                    <button onclick="52.24.156.118:8080/list'">
                        OK
                    </button>
                </center>
            </dialog>
        <%}%>
        <% if (message==2){%>
            <dialog open>
                <center>
                    <h3>
                        Powodzenie!
                    </h3>
                </center>
                <center>
                    <p>
                        Zadanie zostało przyjęte!
                    </p>
                </center>
                <center>
                    <button onclick="52.24.156.118:8080/list'">
                        OK
                    </button>
                </center>
            </dialog>
        <%}%>
        <h2>
            Lista plikow znajdujących się w kubełku:
            <b><%= bucket %></b>
            :
        </h2>
        <div>
            <% for(var i=0; i<fields.length; i++) { %>
                <div>
                    <a href='<%= adresy[i] %>'>
                        <img src="<%= adresy[i] %>" alt="<%=names[i]%>" style="width:90px;height:90px;">
                    </a>
                </div>
                <div>
                    <form id="actionform" name="wartosc" action="52.24.156.118:8080/action" method='get'>
                        <input type="hidden" name='klucz' value="<%= fields[i] %>"/>
                        <input type="submit" value="Obróć"/>
                        <input type="button" onclick="location.href='52.24.156.118:8080/delete?klucz=<%= fields[i] %>'" value="Delete"/>
                    </form>
                </div>
            <% } %>
        </div>
    </body>

</html>
